{
	"name": "chess-study",
	"version": "1.1.0",
	"description": "A chess study helper and PGN viewer/editor.",
	"keywords": [
		"chess",
		"obsidian"
	],
	"license": "GPL-3.0-or-later",
	"author": "Christoph Lindst√§dt",
	"main": "main.js",
	"scripts": {
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"dev": "node esbuild.config.mjs",
		"format": "prettier --write .",
		"prepare": "husky install",
		"deploy": "cp ./main.js ../Chess/.obsidian/plugins/chess-study/main.js && cp ./styles.css ../Chess/.obsidian/plugins/chess-study/styles.css",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"dependencies": {
		"@mliebelt/pgn-parser": "1.4.19",
		"@tiptap/pm": "3.19.0",
		"@tiptap/react": "3.19.0",
		"@tiptap/extension-document": "3.19.0",
		"@tiptap/extension-paragraph": "3.19.0",
		"@tiptap/extension-text": "3.19.0",
		"@tiptap/extension-bold": "3.19.0",
		"@tiptap/starter-kit": "3.19.0",
		"chess.js": "file:../chess.js",
		"chessground": "9.2.1",
		"immer": "^11.1.4",
		"lucide-react": "0.563.0",
		"nanoid": "5.1.6",
		"react": "19.2.4",
		"react-dom": "19.2.4",
		"use-immer": "^0.11.0"
	},
	"devDependencies": {
		"@mliebelt/pgn-types": "1.0.4",
		"@types/node": "25.2.2",
		"@types/react": "19.2.13",
		"@types/react-dom": "19.2.3",
		"@typescript-eslint/eslint-plugin": "5.59.6",
		"@typescript-eslint/parser": "5.59.6",
		"builtin-modules": "5.0.0",
		"esbuild": "0.27.3",
		"eslint-plugin-react": "7.37.5",
		"eslint-plugin-react-hooks": "7.0.1",
		"husky": "9.1.7",
		"obsidian": "1.11.4",
		"prettier": "3.8.1",
		"prettier-plugin-organize-imports": "4.3.0",
		"pretty-quick": "4.2.2",
		"tslib": "2.8.1",
		"tsm": "2.3.0",
		"ts-loader": "9.5.4",
		"ts-node": "10.9.2",
		"typescript": "5.9.3"
	}
}
