{
	"compilerOptions": {
		"baseUrl": ".",
		"inlineSourceMap": true,
		"inlineSources": true,
		"module": "ESNext",
		"target": "es2020",
		"allowJs": true,
		"noImplicitAny": true,
		"moduleResolution": "node",
		"importHelpers": true,
		"isolatedModules": true,
		"strictNullChecks": true,
		"jsx": "react",
		"lib": ["DOM", "ES5", "ES6", "ES7"]
	},
	"include": ["**/*.ts", "src/**/*.tsx"],
	"exclude": ["node-modules"]
}
